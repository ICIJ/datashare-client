{
  "SearchDocumentNavbar": {
    "goToPreviousDocument": {
      "keys": {
        "mac": ["meta", "arrowleft"],
        "default": ["ctrl", "arrowleft"]
      },
      "action": "goToPreviousDocument"
    },
    "goToNextDocument": {
      "keys": {
        "mac": ["meta", "arrowright"],
        "default": ["ctrl", "arrowright"]
      },
      "action": "goToNextDocument"
    },
    "backToSearchResults": {
      "keys": {
        "mac": ["esc"],
        "default": ["esc"]
      },
      "action": "back"
    }
  },
  "DocumentView": {
    "tabNavigation": {
      "keys": {
        "mac": {
          "goToPreviousTab": ["meta", "alt", "arrowdown"],
          "goToNextTab": ["meta", "alt", "arrowup"]
        },
        "default": {
          "goToPreviousTab": ["ctrl", "alt", "pagedown"],
          "goToNextTab": ["ctrl", "alt", "pageup"]
        }
      },
      "action": "shortKeyAction"
    }
  },
  "DocumentLocalSearchInput": {
    "findInDocument": {
      "keys": {
        "mac": {
          "activateSearchBar": ["meta", "f"],
          "deactivateSearchBar": ["esc"],
          "findPreviousOccurrence": ["shift", "enter"],
          "findPreviousOccurrenceAlt": ["shift", "f3"],
          "findNextOccurrence": ["enter"],
          "findNextOccurrenceAlt": ["f3"]
        },
        "default": {
          "activateSearchBar": ["ctrl", "f"],
          "deactivateSearchBar": ["esc"],
          "findPreviousOccurrence": ["shift", "enter"],
          "findPreviousOccurrenceAlt": ["shift", "f3"],
          "findNextOccurrence": ["enter"],
          "findNextOccurrenceAlt": ["f3"]
        }
      },
      "action": "shortkeyAction"
    }
  }
}
