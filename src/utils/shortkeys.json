{
  "SearchDocumentNavbar": {
    "goToPreviousDocument": {
      "keys": {
        "mac": ["meta", "arrowleft"],
        "default": ["ctrl", "arrowleft"]
      },
      "action": "goToPreviousDocument",
      "icon": "backward",
      "label": "shortkeys.goToPreviousDocument",
      "link": "https://icij.gitbook.io/datashare/all/use-keyboard-shortcuts#go-to-the-next-previous-document"
    },
    "goToNextDocument": {
      "keys": {
        "mac": ["meta", "arrowright"],
        "default": ["ctrl", "arrowright"]
      },
      "action": "goToNextDocument",
      "icon": "forward",
      "label": "shortkeys.goToNextDocument",
      "link": "https://icij.gitbook.io/datashare/all/use-keyboard-shortcuts#go-to-the-next-previous-document"
    },
    "backToSearchResults": {
      "keys": {
        "mac": ["esc"],
        "default": ["esc"]
      },
      "action": "back",
      "label": "shortkeys.backToSearchResults",
      "link": "https://icij.gitbook.io/datashare/all/use-keyboard-shortcuts#go-back-to-search-results"
    }
  },
  "DocumentView": {
    "tabNavigation": {
      "keys": {
        "mac": {
          "goToPreviousTab": ["meta", "alt", "arrowdown"],
          "goToNextTab": ["meta", "alt", "arrowup"]
        },
        "default": {
          "goToPreviousTab": ["ctrl", "alt", "pagedown"],
          "goToNextTab": ["ctrl", "alt", "pageup"]
        }
      },
      "action": "shortKeyAction",
      "label": {
        "goToPreviousTab": "shortkeys.goToPreviousTab",
        "goToNextTab": "shortkeys.goToNextTab"
      },
      "link": "https://icij.gitbook.io/datashare/all/use-keyboard-shortcuts#navigate-a-documents-tabs"
    }
  },
  "DocumentLocalSearchInput": {
    "findInDocument": {
      "keys": {
        "mac": {
          "activateSearchBar": ["meta", "f"],
          "deactivateSearchBar": ["esc"],
          "findPreviousOccurrence": ["shift", "enter"],
          "findPreviousOccurrenceAlt": ["shift", "f3"],
          "findNextOccurrence": ["enter"],
          "findNextOccurrenceAlt": ["f3"]
        },
        "default": {
          "activateSearchBar": ["ctrl", "f"],
          "deactivateSearchBar": ["esc"],
          "findPreviousOccurrence": ["shift", "enter"],
          "findPreviousOccurrenceAlt": ["shift", "f3"],
          "findNextOccurrence": ["enter"],
          "findNextOccurrenceAlt": ["f3"]
        }
      },
      "action": "shortkeyAction",
      "label": {
        "activateSearchBar": "shortkeys.activateSearchBar",
        "deactivateSearchBar": "shortkeys.deactivateSearchBar",
        "findPreviousOccurrence": "shortkeys.findPreviousOccurrence",
        "findPreviousOccurrenceAlt": "shortkeys.findPreviousOccurrenceAlt",
        "findNextOccurrence": "shortkeys.findNextOccurrence",
        "findNextOccurrenceAlt": "shortkeys.findNextOccurrenceAlt"
      },
      "link": "https://icij.gitbook.io/datashare/all/use-keyboard-shortcuts#find-in-document"
    }
  }
}
