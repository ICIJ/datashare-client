{
  "name": "datashare-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node --max_old_space_size=4096 node_modules/@vue/cli-service/bin/vue-cli-service.js build",
    "lint": "vue-cli-service lint",
    "test:unit": "TEST_MODE=unit vue-cli-service test:unit --testPathPattern=/unit/ --runInBand",
    "test:unit:majestic": "majestic --noOpen",
    "test:unit:watch": "TEST_MODE=unit vue-cli-service test:unit --testPathPattern=/unit/ --runInBand --watch",
    "test:integration": "TEST_MODE=integration vue-cli-service test:unit --testPathPattern=/integration/ --runInBand",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "yarn test:unit && yarn test:integration && yarn test:e2e"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@icij/murmur": "1.15.0",
    "bodybuilder": "^2.2.17",
    "bootstrap": ">=4.3.1",
    "bootstrap-vue": "^2.0.0-rc.27",
    "cheerio": "^1.0.0-rc.3",
    "elasticsearch-browser": "^16.1.1",
    "es6-promise": "^4.2.8",
    "fetch-ponyfill": "^6.1.0",
    "lodash": "^4.17.15",
    "lucene": "^2.0.4",
    "moment": "^2.21.0",
    "p-queue": "^6.1.0",
    "pdfjs-dist": "^2.1.266",
    "tiff.js": "^1.0.0",
    "tiny-cookie": "^2.1.2",
    "v-runtime-template": "^1.10.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.12.0",
    "vue-infinite-loading": "^2.4.4",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-progressbar": "^0.7.4",
    "vue-router": "^3.0.7",
    "vue-scrollto": "^2.15.0",
    "vue-shortkey": "^3.1.7",
    "vue-sticky-directive": "^0.0.9",
    "vuex": "^3.1.1",
    "vuex-map-fields": "^1.3.4",
    "vuex-persistedstate": "^2.5.4",
    "whatwg-fetch": "^3.0.0",
    "xlsx": "^0.14.4",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-e2e-nightwatch": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "canvas": "^2.5.0",
    "front-matter": "^3.0.2",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-canvas-mock": "^2.1.0",
    "jest-puppeteer": "^4.3.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^9.2.1",
    "markdown-loader": "^5.1.0",
    "marked": "^0.7.0",
    "node-sass": "^4.12.0",
    "puppeteer": "^1.19.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.1",
    "vue-template-compiler": "^2.6.10",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "import/no-webpack-loader-syntax": "off",
      "no-useless-escape": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "majestic": {
    "jestScriptPath": "./node_modules/@vue/cli-service/bin/vue-cli-service.js",
    "env": {
      "TEST_MODE": "unit"
    },
    "args": [
      "test:unit",
      "--config=./jest.config.js",
      "--testPathPattern=/unit/",
      "--runInBand"
    ]
  }
}
