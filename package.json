{
  "name": "datashare-client",
  "version": "0.1.0-canary.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ICIJ/datashare-client/"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node --max_old_space_size=4096 node_modules/@vue/cli-service/bin/vue-cli-service.js build",
    "test:unit": "vue-cli-service test:unit --testPathPattern=/unit/ --maxWorkers=2",
    "test:unit:watch": "vue-cli-service test:unit --testPathPattern=/unit/ --watch",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "test": "yarn test:unit && yarn test:e2e",
    "doc": "yarn doc:api && yarn doc:hooks && yarn doc:components && yarn doc:components:toc && yarn doc:widgets",
    "doc:api": "npx jsdoc-to-markdown --template bin/DOCS.API.hbs src/core/*.js > 'public/docs/client/Client-›-API.md'",
    "doc:hooks": "node bin/collectHooks.js",
    "doc:components": "npm install -g vue@2 vue-docgen-cli@4 && vue-docgen",
    "doc:components:toc": "node bin/collectTocForComponentsDoc.js",
    "doc:widgets": "npx jsdoc-to-markdown src/store/widgets/*.js > 'public/docs/client/Client-›-Widgets.md'"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@icij/murmur": "2.39.8",
    "axios": "^0.27.2",
    "bodybuilder": "^2.2.25",
    "bootstrap": ">=4.5.3",
    "bootstrap-vue": "2.22.0",
    "d3": "^5.15.0",
    "elasticsearch-browser": "16.7.1",
    "es6-promise": "^4.2.8",
    "fuse.js": "^6.6.2",
    "intersection-observer": "^0.12.2",
    "json-formatter-js": "^2.2.1",
    "lodash": "^4.17.19",
    "lucene": "^2.1.0",
    "moment": "^2.29.3",
    "mutationobserver-shim": "^0.3.7",
    "path-to-regexp": "^6.2.1",
    "release": "^6.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "tiff.js": "^1.0.0",
    "tiny-cookie": "^2.1.2",
    "v-calendar": "2.4.1",
    "v-runtime-template": "^1.10.0",
    "vue": "^2.6.14",
    "vue-ellipse-progress": "^1.3.1",
    "vue-i18n": "^8.24.4",
    "vue-infinite-loading": "^2.4.4",
    "vue-perfect-scrollbar": "^0.2.0",
    "vue-progressbar": "^0.7.4",
    "vue-router": "^3.4.9",
    "vue-scrollto": "^2.20.0",
    "vue-shortkey": "^3.1.7",
    "vue-sticky-directive": "^0.0.10",
    "vue-virtual-scroller": "^1.0.0-rc.2",
    "vue-wait": "^1.4.8",
    "vuex": "^3.6.0",
    "vuex-map-fields": "^1.4.1",
    "vuex-persistedstate": "^4.0.0-beta.1",
    "xlsx": "^0.18.5",
    "xss": "^1.0.10"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-e2e-nightwatch": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-require-context": "^0.1.1",
    "canvas": "^2.8.0",
    "eslint": "^7.30.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.13.0",
    "file-loader": "^6.2.0",
    "flush-promises": "^1.0.2",
    "front-matter": "^4.0.2",
    "git-revision-webpack-plugin": "^5.0.0",
    "glob": "^7.1.7",
    "handlebars": "^4.7.6",
    "html-loader": "^1.3.2",
    "isomorphic-fetch": "^3.0.0",
    "jest-canvas-mock": "^2.3.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.5.2",
    "markdown-loader": "^6.0.0",
    "marked": "^2.1.3",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "sass-resources-loader": "^2.2.4",
    "vue-template-compiler": "^2.6.14",
    "whatwg-fetch": "^3.5.0",
    "worker-loader": "^3.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "import/no-webpack-loader-syntax": "off",
      "no-useless-escape": "off",
      "lines-between-class-members": "off",
      "template-curly-spacing": "off",
      "vue/custom-event-name-casing": "off",
      "indent": [
        "error",
        2,
        {
          "ignoredNodes": [
            "TemplateLiteral"
          ]
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint"
    ],
    "*.vue": [
      "vue-cli-service lint"
    ]
  },
  "majestic": {
    "jestScriptPath": "./node_modules/@vue/cli-service/bin/vue-cli-service.js",
    "args": [
      "test:unit",
      "--config=./jest.config.js",
      "--testPathPattern=/unit/",
      "--runInBand"
    ]
  }
}
