{
  "name": "datashare-client",
  "version": "11.2.1",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ICIJ/datashare-client/"
  },
  "scripts": {
    "serve": "vite serve",
    "build": "vite build",
    "test": "yarn test:unit",
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "doc": "yarn doc:api && yarn doc:hooks && yarn doc:widgets",
    "doc:dir": "mkdir -p dist/docs",
    "doc:api": "npx jsdoc-to-markdown --plugin bin/dmd/plugin.cjs --separators --no-gfm --template bin/dmd/api.hbs src/core/*.js > 'dist/docs/api.md'",
    "doc:hooks": "node bin/generateHook.cjs",
    "doc:widgets": "npx jsdoc-to-markdown --plugin bin/dmd/plugin.cjs --separators --no-gfm --template bin/dmd/widgets.hbs src/store/widgets/*.js > 'dist/docs/widgets.md'",
    "predoc": "yarn doc:dir",
    "storybook": "storybook dev -p 6006 --ci",
    "build:storybook": "storybook build"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@icij/murmur-next": "4.4.0",
    "@phosphor-icons/vue": "^2.2.1",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.7.9",
    "bodybuilder": "^2.5.0",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.28.0",
    "d3": "^7.6.1",
    "elasticsearch-browser": "16.7.1",
    "fuse.js": "^7.0.0",
    "image-js": "^0.35.6",
    "intersection-observer": "^0.12.2",
    "json-formatter-js": "^2.5.18",
    "lodash": "^4.17.21",
    "lucene": "^2.1.0",
    "mitt": "^3.0.1",
    "moment": "^2.29.4",
    "mutationobserver-shim": "^0.3.7",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^7.1.0",
    "pinia": "^3.0.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "string-to-color": "^2.2.2",
    "tiff": "^6.1.1",
    "tiny-cookie": "^2.1.2",
    "v-calendar": "3.0.1",
    "v-runtime-template": "^1.10.0",
    "v3-infinite-loading": "^1.3.2",
    "vue": "^3.5.13",
    "vue-i18n": "^9.14.1",
    "vue-multiselect": "3.1.0",
    "vue-router": "^4.4.5",
    "vue-scrollto": "^2.20.0",
    "vue-sticky-directive": "^0.0.10",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vue-wait": "^1.5.3",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vue3-shortkey": "^4.0.0",
    "vue3-toastify": "^0.2.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.5",
    "@icij/eslint-config-icij": "^1.0.2",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-themes": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/vue3": "^8.6.4",
    "@storybook/vue3-vite": "^8.6.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue2": "^2.3.3",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "front-matter": "^4.0.2",
    "handlebars": "^4.7.6",
    "jsdoc-to-markdown": "^8.0.3",
    "jsdom": "^26.0.0",
    "sass": "^1.79.3",
    "storybook": "^8.6.4",
    "storybook-addon-pseudo-states": "^4.0.2",
    "storybook-addon-vue-slots": "^0.9.30-next.2",
    "storybook-vue3-router": "^5.0.0",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.4",
    "whatwg-fetch": "^3.5.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">= 16",
    "npm": ">= 8"
  }
}
