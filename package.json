{
  "name": "datashare-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --runInBand",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "yarn test:unit && yarn test:e2e"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.1",
    "@fortawesome/free-brands-svg-icons": "^5.1",
    "@fortawesome/free-regular-svg-icons": "^5.1.1",
    "@fortawesome/free-solid-svg-icons": "^5.1",
    "@fortawesome/vue-fontawesome": "^0.0.22",
    "bodybuilder": "^2.2.10",
    "bootstrap": "4.0.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "elasticsearch-browser": "^15.1.1",
    "es6-promise": "^4.2.4",
    "fetch-ponyfill": "^6.0.2",
    "handsontable": "^2.0.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "papaparse": "^4.4.0",
    "pdfjs-dist": "^2.0.489",
    "tiff.js": "^1.0.0",
    "tiny-cookie": "^2.1.2",
    "vue": "^2.5.2",
    "vue-headroom": "^0.8.1",
    "vue-i18n": "^7.4.2",
    "vue-progressbar": "^0.7.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.1.6",
    "vuex-persistedstate": "^2.5.1",
    "whatwg-fetch": "^2.0.4",
    "xlsx": "^0.12.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-e2e-nightwatch": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-plugin-unit-jest": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "@vue/eslint-config-standard": "^3.0.0-rc.10",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "canvas": "^1.6.11",
    "chai": "^4.1.2",
    "git-revision-webpack-plugin": "^3.0.3",
    "isomorphic-fetch": "^2.2.1",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.3",
    "sinon": "^6.1.4",
    "vue-template-compiler": "^2.5.16",
    "worker-loader": "^1.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(bootstrap-vue)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.js"
    ],
    "coverageDirectory": "tests/unit/coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.(js|vue)"
    ],
    "testURL": "http://localhost/"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
